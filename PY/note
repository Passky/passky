    pass  //替位 使运行
import math //数学函数
import xx   //其他.py文件 但是无.py后缀
return xx,yy,zz  //return a tuple
def my_abs(x):
    if not isinstance(x, (int, float)):
        raise TypeError('bad operand type')
    if x >= 0:
        return x
    else:
        return -x